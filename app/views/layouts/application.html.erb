<!DOCTYPE html>
<html>
  <head>
    <title>music_app</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
    <%= javascript_importmap_tags %>


        <%= stylesheet_link_tag "https://fonts.googleapis.com/css2?family=Rajdhani:wght@400;500;600;700&display=swap" %>
        <%= stylesheet_link_tag "/assets/stylesheets/bootstrap.min.css" %>
        <%= stylesheet_link_tag "/assets/stylesheets/font-awesome.min.css" %>
        <%= stylesheet_link_tag "/assets/stylesheets/barfiller.css" %>
        <%= stylesheet_link_tag "/assets/stylesheets/nowfont.css" %>
        <%= stylesheet_link_tag "/assets/stylesheets/rockville.css" %>
        <%= stylesheet_link_tag "/assets/stylesheets/magnific-popup.css" %>
        <%= stylesheet_link_tag "/assets/stylesheets/owl.carousel.min.css" %>
        <%= stylesheet_link_tag "/assets/stylesheets/slicknav.min.css" %>
        <%= stylesheet_link_tag "/assets/stylesheets/style.css" %>
        <%= stylesheet_link_tag "https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css", integrity: "sha512-DTOQO9RWCH3ppGqcWaEA1BIZOC6xxalwEsw9c2QQeAIftl+Vegovlnee1c9QX4TctnWMn13TZye+giMm8e2LwA==", crossorigin: "anonymous", referrerpolicy: "no-referrer" %>
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
  </head>

  <body>
    <%= yield %>
     <p class="notice"><%= notice %></p>
     <p class="alert"><%= alert %></p>

        <%= javascript_include_tag 'jquery-3.3.1.min' %>
        <%= javascript_include_tag 'bootstrap.min' %>
        <%= javascript_include_tag 'jquery.magnific-popup.min' %>
        <%= javascript_include_tag 'jquery.nicescroll.min' %>
        <%= javascript_include_tag 'jquery.barfiller' %>
        <%= javascript_include_tag 'jquery.countdown.min' %>
        <%= javascript_include_tag 'jquery.slicknav' %>
        <%= javascript_include_tag 'owl.carousel.min' %>
        <%= javascript_include_tag 'main' %>
        <%= javascript_include_tag 'jquery.jplayer.min' %>
        <%= javascript_include_tag 'jplayerInit' %>
  </body>
</html>
